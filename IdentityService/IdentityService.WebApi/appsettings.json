{
  "ConnectionStrings": {
    "DefaultConnection": "Server=(localdb)\\MSSQLLocalDB;Database=IdentityService;Trusted_Connection=True;MultipleActiveResultSets=true"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "AppSettings": {
    "ApiName": "IdentityService",
    "ApiUrl": "https://localhost:5000",
    "IssuerUri": "https://localhost:5000",
    "PublicOrigin": "https://localhost:5000",
    "LogoutPageUrl": "/#/identity/logout",
    "LoginPageUrl": "/#/identity/login",
    "ResetPasswordUrl": "/#/identity/reset-password",
    "CookiePath": "/",
    "SigninKeyCredentials": "IdentityServerCN",
    "AccessTokenLifetimeInSeconds": 5184000,
    "DataProtection": {
      "IsRequired": false,
      "CertificateIssuerName": "IdentityServer.com",
      "KeyFolderName": "KeyFolder"
    }
  }
}
